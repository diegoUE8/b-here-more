{
  "author": "Luca Zampetti <lzampetti@gmail.com>",
  "name": "b-here-demo",
  "description": "b-here-more",
  "version": "1.0.14",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git://github.com/diegoUE8/b-here-more.git"
  },
  "engines": {
    "node": "^17 || ^16 || ^15 || ^14 || ^13 || ^12 || ^11 || ^10 || ^9 || ^8"
  },
  "main": "index.js",
  "files": [
    "server/**/*.*",
    "dist/**/*.*",
    "!dist/css/lib/*",
    "!dist/js/lib/*"
  ],
  "scripts": {
    "cert:ca": "mkcert create-ca",
    "cert:cert": "mkcert create-cert",
    "cert": "run-s cert:*",
    "serve": "node index.js",
    "run": "gulp build --target vendors && gulp build && npm run serve",
    "build": "gulp build",
    "dist": "gulp build --target dist",
    "watch": "gulp start",
    "start": "run-p watch serve",
    "test": "node test/selenium-test.js"
  },
  "dependencies": {
    "beta-bhere-development": "1.0.14"
  },
  "devDependencies": {
    "gulpfile-config": "1.0.0-alpha.18",
    "mkcert": "1.5.0",
    "npm-run-all": "4.1.5",
    "selenium-webdriver": "4.0.0-beta.1"
  }
}
